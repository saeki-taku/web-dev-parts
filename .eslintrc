{
  "parser": "babel-eslint",
  "extends": "airbnb-base",
  "rules": {
    "no-undef": 0,
    "no-alert": 0,
    "no-use-before-define": 0,
    "no-unused-vars": 0,
    "no-console": 0,
    "quotes": 0,
    "no-restricted-syntax": 0,
    "object-curly-newline": 0,
    "prefer-destructuring": 0,
    "prefer-const": 0
    // "strict": 0,
  },
  "globals": {
    "$": false
  },
  "env": {
    "jquery": true,
    "browser": true
  }
}
